        "foregroundImage": "./assets/adaptive-icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.zeeshandev099.smartcalculator"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.jpg"
    },
    "extra": {
      "eas": {
        "projectId": "dd1bc9c5-2255-4b35-acfe-a69e9885c078"
      }
    }
  }
}
